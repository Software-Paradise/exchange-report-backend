{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source":{
        "include": ["api"],
        "exclude": [ "node_modules" ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown", "jsdoc-route-plugin"],
    "templates":{
        "cleverLinks": false,
        "monospaceLinks": true
    },
    "opts":{
        "recurse": true,
        "destination": "./docs/"
    }
}